#qrCode.modal.fade{ tabindex: '-1' }
  .modal-dialog.modal-dialog-centered
    .modal-content
      .modal-header
        %h5.modal-title= I18n.t('qr')
        %button.close{ type: 'button', 'data-dismiss': 'modal' }
          &times;
      .modal-body.text-center
        = @qr.html_safe
        %p= "pintu:#{@user.email}"
      .modal-footer
        %p= I18n.t('qr_info')
